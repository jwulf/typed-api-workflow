{
  "name": "full",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start:trace": "CAMUNDA_TRACE_REQUESTS=true ts-node full-stack.ts",
    "start": "OTEL_TRACING_ENABLED=false ts-node full-stack.ts",
    "test": "npm run start",
    "signal": "CAMUNDA_SDK_VALIDATION=none && cd signal && ts-node signal.ts",
    "signal:strict": "export CAMUNDA_SDK_VALIDATION=strict && cd signal &&  ts-node signal.ts",
    "signal:deploy": "cd signal && ts-node deploy.ts",
    "demo:logging": "ts-node demo-logging-levels.ts",
    "demo:trace-basic": "CAMUNDA_TRACE_REQUESTS=true CAMUNDA_TRACE_LEVEL=basic npm run start",
    "demo:trace-detailed": "CAMUNDA_TRACE_REQUESTS=true CAMUNDA_TRACE_LEVEL=detailed npm run start",
    "demo:trace-none": "CAMUNDA_TRACE_REQUESTS=true CAMUNDA_TRACE_LEVEL=none npm run start",
    "lint": "eslint --config eslint.config.js ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "dependencies": {
    "@camunda8/sdk": "^8.7.28",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/sdk-node": "^0.203.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@types/node": "^24.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@typescript-eslint/utils": "^8.39.1",
    "eslint": "^8.57.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "typescript-eslint": "^8.39.1"
  }
}
